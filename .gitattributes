models/** filter=lfs diff=lfs merge=lfs -text
dataset/** filter=lfs diff=lfs merge=lfs -text
models/pretrained/** filter=lfs diff=lfs merge=lfs -text
models/custom/** filter=lfs diff=lfs merge=lfs -text
models/optimized/** filter=lfs diff=lfs merge=lfs -text
dataset/preset/** filter=lfs diff=lfs merge=lfs -text
dataset/custom/** filter=lfs diff=lfs merge=lfs -text

# Force LF line endings for all text files
* text=auto eol=lf

# Python files
*.py text eol=lf

# JavaScript/TypeScript files
*.js text eol=lf
*.jsx text eol=lf
*.ts text eol=lf
*.tsx text eol=lf

# Config files
*.json text eol=lf
*.yml text eol=lf
*.yaml text eol=lf
*.toml text eol=lf
*.md text eol=lf

# Shell scripts
*.sh text eol=lf

# Docker files
Dockerfile text eol=lf